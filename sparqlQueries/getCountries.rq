PREFIX dct: <http://purl.org/dc/terms/>
PREFIX ejp: <http://purl.org/ejp-rd/vocabulary/>


SELECT DISTINCT ?value WHERE {

  ?baseContainer ldp:contains ?catalogContainer .

  GRAPH ?catalogContainer {
    ?catalog a ejp:CatalogOfRegistries .
    ?catalogContainer ldp:contains ?registryContainer
  }
  GRAPH ?registryContainer {
    ?registry dct:publisher [ dct:spatial [ ejp:country ?countryStr] ]
    FILTER (?value != "")
   
   BIND(lcase(?countryStr) AS ?value)  

  }
} ORDER BY ?value